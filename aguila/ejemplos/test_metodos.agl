# Test métodos nativos optimizados
numeros = [5, 2, 8, 1, 9, 3]

imprimir "Lista:"
imprimir numeros

# Suma
total = numeros.suma()
imprimir a"Suma: {total}"

# Mínimo
menor = numeros.minimo()
imprimir a"Mínimo: {menor}"

# Máximo
mayor = numeros.maximo()
imprimir a"Máximo: {mayor}"

# Longitud
tam = numeros.longitud()
imprimir a"Longitud: {tam}"

# Promedio (usando suma y longitud)
promedio = numeros.suma() / numeros.longitud()
imprimir a"Promedio: {promedio}"
