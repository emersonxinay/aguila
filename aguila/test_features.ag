# Test de Listas
imprimir("--- Listas ---");

lista = [1, 2, 3];
imprimir(lista);
imprimir(lista[0]);
lista[1] = 99;
imprimir(lista);

# Test de Diccionarios
imprimir("\n--- Diccionarios ---");
dict = {"a": 1, "b": 2};
imprimir(dict);
imprimir(dict["a"]);
dict["c"] = 3;
imprimir(dict);

# Test de Clases
imprimir("\n--- Clases ---");
clase Persona {
    funcion init(nombre) {
        yo.nombre = nombre;
        imprimir("Init ejecutado para " + nombre);
    }
    funcion saludar() {
        imprimir("Hola " + yo.nombre);
    }
}

imprimir("Llamar: callee=<Clase Persona>");
p = Persona("Mundo");
p.saludar();

# Test de Try/Catch
imprimir("\n--- Try/Catch ---");
intentar {
    imprimir("Intentando algo peligroso...");
    lanzar "Boom!";
} capturar Error como error {
    imprimir("Capturado: " + error);
} finalmente {
    imprimir("Finalmente ejecutado");
}
